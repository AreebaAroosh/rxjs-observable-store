{
  "name": "rxjs-observable-store",
  "version": "1.0.1",
  "description": "A state management solution implemented using RxJS to mimic Redux architecture.",
  "keywords": [
    "rxjs",
    "observable",
    "store",
    "redux",
    "angular",
    "state management"
  ],
  "license": "MIT",
  "author": "Jure Bajt",
  "homepage": "https://jurebajt.com/state-management-in-angular-with-observable-store-services/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jurebajt/rxjs-observable-store.git"
  },
  "bugs": {
    "url": "https://github.com/jurebajt/rxjs-observable-store/issues"
  },
  "main": "./lib/index.js",
  "typings": "./lib/index.d.ts",
  "scripts": {
    "test": "jest",
    "build": "rimraf lib/**/* && tsc",
    "prepare": "npm run build"
  },
  "files": [
    "lib"
  ],
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "\\.ts$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "/src/.*\\.spec\\.ts$"
  },
  "peerDependencies": {
    "rxjs": "6.x"
  },
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "jest": "^23.0.1",
    "rimraf": "^2.6.2",
    "rxjs": "^6.2.0",
    "ts-jest": "^22.4.6",
    "typescript": "^2.8.3"
  }
}
